{
  "name": "avatar-star",
  "description": "",
  "version": "0.1.0",
  "dependencies": {
    "aws-sdk": "^2.877.0",
    "cheerio": "^1.0.0-rc.5",
    "got": "^11.8.2",
    "serverless-prune-plugin": "^1.4.4",
    "sharp": "^0.28.0",
    "tunnel": "0.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/aws-lambda": "^8.10.61",
    "@types/tunnel": "0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "serverless": "^2.35.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-offline": "^6.5.0",
    "serverless-offline-env": "^0.2.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-s3-local": "^0.6.14",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "local-server": "npx serverless offline --stage offline",
    "deploy": "npx serverless deploy --stage dev",
    "lint": "eslint --fix --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build",
    "prepare": "husky install",
    "start": "sls offline"
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "npm run lint"
    ],
    "*.+(js|json|ts|tsx)": [
      "npm run format"
    ]
  }
}
